{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:ListBucket",
        "s3:ListBucketVersions",
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": [
        "arn:aws:s3:::certshack-examapp-questions",
        "arn:aws:s3:::certshack-examapp-questions/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "dynamodb:PutItem",
        "dynamodb:GetItem",
        "dynamodb:Query",
        "dynamodb:Scan",
        "dynamodb:UpdateItem",
        "dynamodb:DeleteItem"
      ],
      "Resource": [
        "arn:aws:dynamodb:eu-west-1:030461496359:table/examapp-attempts",
        "arn:aws:dynamodb:eu-west-1:030461496359:table/examapp-gamification",
        "arn:aws:dynamodb:eu-west-1:030461496359:table/examapp-exams-index",
        "arn:aws:dynamodb:eu-west-1:030461496359:table/examapp-users",
        "arn:aws:dynamodb:eu-west-1:030461496359:table/examapp-entitlements",
        "arn:aws:dynamodb:eu-west-1:030461496359:table/examapp-audit"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "kms:Encrypt",
        "kms:Decrypt",
        "kms:GenerateDataKey",
        "kms:DescribeKey"
      ],
      "Resource": "*"
    }
  ]
}
