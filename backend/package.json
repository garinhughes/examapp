{
  "name": "examapp-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm src/index.ts",
    "dev:reload": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "publish:exams": "node --loader ts-node/esm scripts/publishExam.ts --all",
    "publish:exams:dry": "node --loader ts-node/esm scripts/publishExam.ts --all --dry-run",
    "test:workflow": "node --loader ts-node/esm scripts/testWorkflow.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.988.0",
    "@aws-sdk/client-s3": "^3.991.0",
    "@aws-sdk/lib-dynamodb": "^3.988.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/jwt": "^10.0.0",
    "dotenv": "^17.2.4",
    "fastify": "^4.25.0",
    "fastify-plugin": "^5.1.0",
    "jose": "^6.1.3",
    "jwks-rsa": "^3.2.2"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
